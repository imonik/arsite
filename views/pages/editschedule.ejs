<body>

<div class="super_container">
	
	<!-- Header -->

	<header class="header">
		<div class="container">
			<div class="row">
				<div class="col">
					<div class="header_content d-flex flex-row align-items-center justify-content-start trans_400">
						<a href="#">
							<div class="logo d-flex flex-row align-items-right justify-content-start"><div>AR<span>Pole dance & fitness Horario</span></div></div>
						</a>
						<p>Hello <% if (data && data.user) { %>
							<h2><%= data.user.admin_name %></h2>
						  <% } %></p>
					</div>
				</div>
			</div>
		</div>
	</header>

	<!-- Hamburger -->
	
	<div class="hamburger_bar trans_400 d-flex flex-row align-items-center justify-content-start">
		<div class="hamburger">
			<div class="menu_toggle d-flex flex-row align-items-center justify-content-start">
				<span>menu</span>
				<div class="hamburger_container">
					<div class="menu_hamburger">
						<div class="line_1 hamburger_lines" style="transform: matrix(1, 0, 0, 1, 0, 0);"></div>
						<div class="line_2 hamburger_lines" style="visibility: inherit; opacity: 1;"></div>
						<div class="line_3 hamburger_lines" style="transform: matrix(1, 0, 0, 1, 0, 0);"></div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<!-- Menu -->

	<div class="menu trans_800">
		<div class="menu_content d-flex flex-column align-items-center justify-content-center text-center">
		</div>
	</div>
	<!-- Home -->
	<div class="home">
		<div class="background_image" style="background-image:url(/assets/images/extra_wide.jpg)"></div>
		<div class="overlay"></div>
		<div class="home_container">
			<div class="container">
				<div class="row">
					<div class="col">
						<h1 class="text-light text-center">Modificar Clase</h1>

						<div class="container-form withe_text mb-2">
							<div class="expand-collapse"><h1><span>-</span></h1>
							</div>
							<div id="message"></div>
							<div class="content">
								<form>
									<div class="form-group d-inline"> 
										<label for="clase">Clase</label>
										<input type="text" class="form-control-sm" id="clase" value="<%= data.schedule.name%>">
										<label for="instructor">Instructor</label>
                                        
                                        <select class="form-control-sm" id="instructor">
                                        </select>
                                        
										<label for="start_time"  class="mr-1">Dia</label>
										<input type='text' class="form-control-sm"   id='date' value="<%= data.schedule.date%>"/>
									
										<label for="start_time"  class="mr-1">De</label>
										<select class="form-control-sm " id="start_time">
												<option selected>Inicio</option>
												<option value="8:00">8:00</option>
												<option value="8:30">8:30</option>
												<option value="9:00">9:00</option>
												<option value="9:30">9:30</option>
												<option value="10:00">10:00</option>
												<option value="10:30">10:30</option>
												<option value="11:00">11:00</option>
												<option value="11:30">11:30</option>
												<option value="12:00">12:00</option>
												<option value="12:30">12:30</option>
												<option value="13:00">13:00</option>
												<option value="13:30">13:30</option>
												<option value="14:00">14:00</option>
												<option value="14:30">14:30</option>
												<option value="15:00">15:00</option>
												<option value="15:30">15:30</option>
												<option value="16:00">16:00</option>
												<option value="16:30">16:30</option>
												<option value="17:00">17:00</option>
												<option value="17:30">17:30</option>
												<option value="18:00">18:00</option>
												<option value="18:30">18:30</option>
												<option value="19:00">19:00</option>
												<option value="19:30">19:30</option>
												<option value="20:00">20:30</option>
												<option value="20:30">20:30</option>						
																							</select>

										<label for="end_time"  class="mr-1 d-inline">a</label>
										<select class="form-control-sm d-inline" id="end_time">
												<option selected>Fin</option>
												<option value="8:00">8:00</option>
												<option value="8:30">8:30</option>
												<option value="9:00">9:00</option>
												<option value="9:30">9:30</option>
												<option value="10:00">10:00</option>
												<option value="10:30">10:30</option>
												<option value="11:00">11:00</option>
												<option value="11:30">11:30</option>
												<option value="12:00">12:00</option>
												<option value="12:30">12:30</option>
												<option value="13:00">13:00</option>
												<option value="13:30">13:30</option>
												<option value="14:00">14:00</option>
												<option value="14:30">14:30</option>
												<option value="15:00">15:00</option>
												<option value="15:30">15:30</option>
												<option value="16:00">16:00</option>
												<option value="16:30">16:30</option>
												<option value="17:00">17:00</option>
												<option value="17:30">17:30</option>
												<option value="18:00">18:00</option>
												<option value="18:30">18:30</option>
												<option value="19:00">19:00</option>
												<option value="19:30">19:30</option>
												<option value="20:00">20:30</option>
												<option value="20:30">20:30</option>
										</select>
                                    </div>
                                    <input type="hidden" id='hdnInstructorId' value='<%= data.schedule.instructor_id%>'>
                                    <input type="hidden" id='hdnStart' value='<%= data.schedule.start%>'>
																		<input type="hidden" id='hdnEnd' value='<%= data.schedule.end%>'>
																		<input type="hidden" id='hdnUid' value='<%= data.schedule.id%>'>
																		<input type="hidden" id="isEdit" value="<%= data.schedule.id? true:false%>"> 
								</form>
								<button id="btn-update-schedule"  class="btn btn-secondary mt-2 mb-2">Guardar Cambios</button>
							</div>
                        </div>
						
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- Footer -->
	<footer class="footer">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 offset-lg-2">
